<div class="container-fluid">
  <h1>Login</h1>
  <form action='/sessions' method='post'>
    Email:<input type='text' name='email'>
    Password:<input type='password' name='password'>
    <input type='submit' value='Login'>
    <% if @error %>
      <p><%= @error %></p>
    <% end %>
  </form>
</div>
